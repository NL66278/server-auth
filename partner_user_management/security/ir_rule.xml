<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">

    <record id="rule_res_partner_user_management_read" model="ir.rule">
        <field name="name">Partner User Management: read access</field>
        <field name="model_id" ref="model_partneri_user_management"/>
        <field
            name="groups"
            eval="[(6, False, [ref('base.group_partner_manager')])]"
            />
        <field
            name="domain_force"
            >['|',('partner_manager_ids','=',False),
            ('partner_manager_ids','=',user.partner_id.id)]
        </field>
        <field name="perm_read" eval="True"/>
        <field name="perm_create" eval="False"/>
        <field name="perm_write" eval="False"/>
        <field name="perm_unlink" eval="False"/>
    </record>

</odoo>
